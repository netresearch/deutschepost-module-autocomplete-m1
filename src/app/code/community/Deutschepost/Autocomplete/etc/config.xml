<config>
    <modules>
        <Deutschepost_Autocomplete>
            <version>0.1.0</version>
        </Deutschepost_Autocomplete>
    </modules>
    <global>
        <helpers>
            <deutschepost_autocomplete>
                <class>Deutschepost_Autocomplete_Helper</class>
            </deutschepost_autocomplete>
        </helpers>
        <models>
            <deutschepost_autocomplete>
                <class>Deutschepost_Autocomplete_Model</class>
                <resourceModel>Deutschepost_Autocomplete_resource</resourceModel>
            </deutschepost_autocomplete>
        </models>
    </global>
    <default>
        <deutschepost_autocomplete>
            <username>foo</username>
            <password>baar</password>
            <logging_enabled>1</logging_enabled>
        </deutschepost_autocomplete>
    </default>
</config>